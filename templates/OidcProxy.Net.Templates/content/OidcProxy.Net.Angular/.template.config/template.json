{
    "$schema": "http://json.schemastore.org/template",
    "author": "Albert Starreveld",
    "classifications": [ "Angular", "ASP.NET Core", "C#", "OidcProxy.Net" ],    
    "identity": "OidcProxy.Net.Angular",
    "name": "A boilerplate OidcProxy.Net proxy project with Angular",
    "shortName": "OidcProxy.Net.Angular",
    "sourceName": "MyApp",
    "symbols": {
        "clientId": {
            "type": "parameter",
            "datatype": "string",
            "replaces": "{client_id}",
            "isRequired": "true"
        },
        "clientSecret": {
            "type": "parameter",
            "datatype": "string",
            "replaces": "{client_secret}",
            "isRequired": "true"
        },
        "idp": {
            "type": "parameter",
            "datatype": "string",
            "replaces": "https://{address_to_idp}/",
            "isRequired": "true"
        },
        "backend": {
            "type": "parameter",
            "datatype": "string",
            "replaces": "http://localhost:8080/",
            "isRequired": "true"
        }
    }
}